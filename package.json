{
  "name": "nools-syntax-highlighter",
  "displayName": "Nools Syntax Highlighter",
  "description": "Nools DSL Syntax Highlighter",
  "version": "0.0.1",
  "publisher": "dbrunonascimento",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "vsce": "node_modules/.bin/vsce"
  },
  "contributes": {
    "languages": [
      {
        "id": "nools",
        "aliases": [
          "Nools DSL",
          "nools"
        ],
        "extensions": [
          ".nools"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nools",
        "scopeName": "source.nools",
        "path": "./syntaxes/nools.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^1.75.0"
  }
}
